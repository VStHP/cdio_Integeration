<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">
      <%= t "tasks.task.box_title" %> : <%= @user_tasks.size %> <%= t "tasks.task.text" %>
    </h3>
  </div>
  <div class="box-header with-border">
    <div class="col-md-3">
      <div id="hidden-div"></div>
    </div>
  </div>
  <div class="box-body">
    <%= render partial: "component/task_component/task_item", collection: @user_tasks, as: :user_task%>
  </div>
</div>
