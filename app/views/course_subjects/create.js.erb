<% if params[:ids].present? %>
  <% if @ids_deny.present? %>
    <% @name = String.new %>
    <% @ids_deny.each do |x| %>
      <% @name << Subject.find(x).name << ", " %>
      alertify.info('Some subject insert into course unsuccess: <%= @name %>');
    <% end %>
  <% end %>
  <% @name = String.new %>
  <% (params[:ids]-@ids_deny).each do |x| %>
    $('#subject-adding-<%= x %>').remove();
    <% @name << Subject.find(x).name << ", " %>
  <% end %>
  <% @course_subjects.each do |cs| %>
    $('#subject-added').append('<%= j (render partial: "subject_added", locals: {coursesubject: cs}) %>');
  <% end %>
  alertify.success('<%= @mes_success %> : <%= @name %>');
<% else %>
  alertify.log('<%= @mes_success %>');
<% end %>
  $('#img-load').hide();
